{
  "name": "arsnova-webclient",
  "private": true,
  "license": "MIT",
  "packageManager": "yarn@4.12.0",
  "engines": {
    "yarn": "4.12.0"
  },
  "scripts": {
    "postinstall": "sh -c 'if [ -z \"$CI\" ]; then pipx install pre-commit && pre-commit install; fi'",
    "g:ng": "cd $INIT_CWD && ng",
    "g:eslint": "cd $INIT_CWD && eslint",
    "g:prettier": "cd $INIT_CWD && prettier",
    "g:stylelint": "cd $INIT_CWD && stylelint",
    "start": "ng serve --proxy-config proxy.conf.json",
    "build": "ng build --configuration production",
    "test": "ng test --watch=false --browsers=ChromeHeadlessCustom",
    "lint": "ng lint",
    "storybook": "yarn ng run arsnova-webclient:storybook",
    "build-storybook": "yarn ng run arsnova-webclient:build-storybook",
    "e2e": "yarn playwright test -c e2e/playwright.config.ts",
    "generate-gql": "graphql-codegen"
  },
